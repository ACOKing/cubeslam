		<property name="pan" type="int" default="0" min="-100" max="100" />
		<property name="loop" type="int" unit="mS" default="-1" min="-1" max="100000" /> <!-- -1=off, 0=sample end loop, >0 = loop lenght in mS -->
		<property name="reTrig" type="int" unit="mS" default="-1" min="-1" max="100000" /> <!-- -1=off, 0=on, >0 = reTrig block in mS -->
		<property name="returnEvent" type="string" default="undefined" /> <!-- not sure about this -->
		<property name="returnEventTime" type="int" unit="mS" default="-1" min="-1000000" max="100000" /> <!-- -1=off, 0=on, >0 = reTrig block in mS -->
		<property name="preListen" type="int" default="0" min="0" max="500" />
		<property name="bus" type="string" default="master" />
		<property name="priority" type="boolean" default="false" />
		<property name="timingCorrection" type="enum" default="PLAY" values="RESYNC,SYNC,PLAY"/>
	</descriptor>
    <descriptor class="action" type="sound" id="stepPlay" method="play">
